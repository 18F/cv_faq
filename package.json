{
  "name": "federalist-uswds-jekyll",
  "version": "1.3.0",
  "scripts": {
    "build": "npm run build:site",
    "build:site": "bundle exec jekyll build",
    "clean": "bundle exec jekyll clean",
    "start": "npm run start:site",
    "start:site": "bundle exec jekyll serve --host 0.0.0.0",
    "test": "npm run build && bundle exec htmlproofer _site --empty-alt-ignore"
  },
  "dependencies": {
    "accessible-autocomplete": "^2.0.2",
    "fuse.js": "^5.0.5-beta",
    "netlify-cms": "^2.10.23",
    "uswds": "~2.1.0"
  },
  "devDependencies": {
    "a11y": "^0.5.1"
  }
}
