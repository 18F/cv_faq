{
  "name": "federalist-uswds-jekyll",
  "version": "1.3.0",
  "scripts": {
    "build": "npm run build:scripts && npm run build:site",
    "build:scripts": "webpack --colors --mode production",
    "build:site": "bundle exec jekyll build",
    "clean": "bundle exec jekyll clean",
    "federalist": "npm run build:scripts",
    "start": "run-p start:*",
    "start:scripts": "webpack --colors --watch --mode development",
    "start:site": "bundle exec jekyll serve --host 0.0.0.0",
    "test": "npm run build && npm run test:htmlproofer && npm run test:jest",
    "test:htmlproofer": "bundle exec rake htmlproof",
    "test:jest": "jest"
  },
  "dependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "accessible-autocomplete": "^2.0.2",
    "babel-loader": "^8.1.0",
    "babel-plugin-template-html-minifier": "^4.1.0",
    "core-js": "^3.6.5",
    "fuse.js": "^6.0.3",
    "lit-html": "^1.2.1",
    "netlify-cms": "^2.10.50",
    "npm-run-all": "^4.1.5",
    "serve": "^11.3.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "uswds": "~2.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@apidevtools/swagger-parser": "^9.0.1",
    "a11y": "^0.5.1",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.21.2",
    "fast-xml-parser": "^3.17.4",
    "front-matter": "^4.0.2",
    "jest": "^26.0.1",
    "jest-puppeteer": "^4.4.0",
    "puppeteer": "^2.1.1",
    "true-case-path": "^2.2.1"
  },
  "browserslist": [
    ">0.1%",
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Edge versions",
    "last 2 Samsung versions",
    "Safari >= 9",
    "ie 8-11",
    "iOS >= 9"
  ]
}
