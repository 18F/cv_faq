---
layout: null
---
[
  {% for content in site.content %}
    {
      "title"    : "{{ content.title | escape }}",
      "desc"     : "{{ content.slug }}",
      "category" : "{{ content.category }}",
      "excerpt"  : "{{ content.excerpt | strip_html | strip }}",
      "url"      : "{{ site.baseurl }}{{ content.category }}.html#{{ content.slug }}",
      "date"     : "{{ content.date }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]
